zahlen_v6 = (15..20).to_a
f = {}
square = Hash.new
counter = 0
zahlen_v6.size.times do 
	f = Hash[zahlen_v6[counter], rand(100)+100]
	f.each 
		{ |key,value| puts "Index: #{key} - Wert: #{value}"; value[counter] = value * value;square = Hash[value[counter], value[counter]] square.each {|key,value| puts "Index: #{key} - Wert: #{value}"}}
	counter = counter + 1
end